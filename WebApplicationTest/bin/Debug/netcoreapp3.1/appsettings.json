{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source= .\\SGEDB.db;",
    "providerName": "System.Data.SQLite.EF6"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Swagger": {
    "SwaggerUIOptions": {
      "RoutePrefix": "swagger",
      "DocumentTitle": "SGE",
      "ConfigObject": {
        "Urls": [
          {
            "Url": "v1/swagger.json",
            "Name": "V1 Docs"
          }
        ],
        "SupportedSubmitMethods": [ "Get", "Put", "Post", "Delete" ],
        "DeepLinking": true,
        "DisplayOperationId": false,
        "DefaultModelsExpandDepth": 0,
        "DefaultModelExpandDepth": 2,
        "DefaultModelRendering": "Example", //Example,model
        "DocExpansion": "List", //List,Full,None
        "Filter": "",
        "ShowExtensions": true
      }
    }
  },
  "AllowedHosts": "*"
}
